@model List<ItemCounts>

<div class="container-home">
    <div class="container">
        <div class="row d-flex h-100">
            <div class="col-sm main-screen shadow p-3 mb-5 d-flex flex-column align-items-center pe-none">
                <h4 id="StudentsMaxedOut" class="counter-shadow p-4 mb-3" style="border-radius:50%; width:auto">
                    @TempData["StudentMaxout"]
                </h4>
                <h3>Students MaxedOut</h3>
            </div>
            <div class="col-sm main-screen shadow p-3 mb-5 d-flex flex-column align-items-center pe-none">
                <h4 id="CoursesMaxedOut" class="counter-shadow p-4 mb-3" style="border-radius:50%; width:auto">
                    @TempData["CourseMaxOut"]
                </h4>
                <h3>Courses MaxedOut</h3>
                
            </div>
        </div>
    </div>
</div>